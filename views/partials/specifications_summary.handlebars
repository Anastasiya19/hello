<div class="message message_vinci animated fadeInLeft">

    <div class="owl-carousel owl-theme">

        \{{#each phones}}
        <div class="message__item message__item_vinci br-chat message__category category view_product">
            <button class="button helpers__item view_product__button" onclick="get_specifications_full(event)">View product</button>
            <div class="row">
                <div class="category__image col-4">
                    <img src="\{{this.selected_variant.image_urls.small}}" alt="">
                </div>
                <div class="category__description col-8">
                    <h4 class="category__type">\{{this.selected_variant.product_basic_info.productBrand_normalized}}</h4>
                    <h3 class="category__name">\{{title}}</h3>
                    <div class="category__prices-box">
                        <span>Rs. \{{this.cheapest_price}}</span>
                        <span class="old">Rs.\{{this.other_price}}</span>
                    </div>
                    <div class="facebook-status characteristic__facebook-status">
                        <ul class="facebook-status__list">
                            <li class="facebook-status__item like_reaction">
                                <a href="#" class="facebook-status__link" onclick="react(event,'like','\{{this.selected_variant.product_basic_info.normalized_name}}')">
                                    <img src="assets/build/assets/images/facebook-like.svg" alt="" class="facebook-status__icon">
                                </a>
                                <span>\{{{get_reactions this.selected_variant "like"}}}</span>
                            </li>
                            <li class="facebook-status__item love_reaction">
                                <a href="#" class="facebook-status__link" onclick="react(event,'love','\{{this.selected_variant.product_basic_info.normalized_name}}')">
                                    <img src="assets/build/assets/images/facebook-love.svg" alt="" class="facebook-status__icon">
                                </a>
                                <span>\{{{get_reactions this.selected_variant "love"}}}</span>
                            </li>
                            <li class="facebook-status__item angry_reaction">
                                <a href="#" class="facebook-status__link" onclick="react(event,'angry','\{{this.selected_variant.product_basic_info.normalized_name}}')">
                                    <img src="assets/build/assets/images/facebook-angry.svg" alt="" class="facebook-status__icon">
                                </a>
                                <span>\{{{get_reactions this.selected_variant "angry"}}}</span>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        \{{/each}}
      
   
   <div class="message__item message__item_vinci br-chat message__category category view_product" style="background-color: grey; opacity: 0.8">
        <button class="button helpers__item view_product__button" onclick="get_more(event)" data-skip='\{{skip}}' data-query='\{{query_object}}' style="display: block!important">View more phones</button>
        <div class="row">
            <div class="category__image col-4" style="visibility: hidden;">
                <img src="https://rukminim1.flixcart.com/image/400/400/j7ksia80/mobile/m/y/d/apple-iphone-8-mq6l2hn-a-original-imaexsbz8fggz59j.jpeg?q=90" alt="" class="mCS_img_loaded">
            </div>
            <div class="category__description col-8" style="  visibility:  hidden;">
                <h4 class="category__type">Apple</h4>
                <h3 class="category__name">Apple iPhone 8 (Silver, 256 GB)</h3>
                <div class="category__prices-box">
                    <span>Rs.69848</span>
                    <span class="old">Rs.77000</span>
                </div>
                <div class="facebook-status characteristic__facebook-status">
                    <ul class="facebook-status__list">
                        <li class="facebook-status__item like_reaction">
                            <a href="#" class="facebook-status__link" onclick="react(event,'like','Apple iPhone 8')">
                                <img src="assets/build/assets/images/facebook-like.svg" alt="" class="facebook-status__icon mCS_img_loaded">
                            </a>
                            <span>1</span>
                        </li>
                        <li class="facebook-status__item love_reaction">
                            <a href="#" class="facebook-status__link" onclick="react(event,'love','Apple iPhone 8')">
                                <img src="assets/build/assets/images/facebook-love.svg" alt="" class="facebook-status__icon mCS_img_loaded">
                            </a>
                            <span>1</span>
                        </li>
                        <li class="facebook-status__item angry_reaction">
                            <a href="#" class="facebook-status__link" onclick="react(event,'angry','Apple iPhone 8')">
                                <img src="assets/build/assets/images/facebook-angry.svg" alt="" class="facebook-status__icon mCS_img_loaded">
                            </a>
                            <span>1</span>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
   
    </div>

</div>